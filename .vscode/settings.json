{
  "[dart]": {
    "editor.indentSize": 2,
    "editor.defaultFormatter": "Dart-Code.dart-code",
    "editor.formatOnSave": true,
    "editor.codeActionsOnSave": {
      "source.fixAll": "always",
      "source.organizeImports": "explicit"
    }
  },
  "files.exclude": {
    "**/*.freezed.dart": true,
    "**/*.g.dart": true
  }
}
